{
    "name": "peersend",
    "version": "1.0.0",
    "main": "src/index.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Xmader/PeerSend.git"
    },
    "author": "Xmader",
    "bugs": {
        "url": "https://github.com/Xmader/PeerSend/issues"
    },
    "homepage": "https://github.com/Xmader/PeerSend#readme",
    "scripts": {
        "build": "parcel build src/index.js -d dist --public-url ./ --detailed-report",
        "watch": "parcel watch src/index.js -d dist --public-url ./ ",
        "http-server": "http-server . -p 8082 -c-1",
