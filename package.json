    },
    "devDependencies": {
        "@babel/core": "^7.5.5",
        "@babel/plugin-transform-runtime": "^7.5.5",
        "@types/file-saver": "^2.0.1",
        "@vue/component-compiler-utils": "^2.6.0",
        "core-js": "^3.1.4",
        "http-server": "^0.11.1",
        "parcel-bundler": "^1.12.3",
        "text-encoding": "^0.7.0",
        "typescript": "^3.5.3",
        "vue-hot-reload-api": "^2.3.3",
        "vue-template-compiler": "^2.6.10",
